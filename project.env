# Database configuration for PostgreSQL (running in container called "database-server-dab-p2-bf006f69-136b-4835-8bca-f55abd81c2fa")
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_DB=database

# Database configuration for Flyway (used for database migrations)
FLYWAY_USER=username
FLYWAY_PASSWORD=password
FLYWAY_URL=jdbc:postgresql://database-server-dab-p2-bf006f69-136b-4835-8bca-f55abd81c2fa:5432/database

# Database configuration for PostgreSQL driver
PGUSER=username
PGPASSWORD=password
PGHOST=database-server-dab-p2-bf006f69-136b-4835-8bca-f55abd81c2fa
PGPORT=5432
PGDATABASE=database

# Deno cache location (avoid reloading depedencies over and over)
DENO_DIR=/app-cache